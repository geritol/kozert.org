{
  "name": "kozert.org",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db": "CONTAINER_NAME=kozert-db PORT=54320 bash ./scripts/start-db.sh",
    "db:integration": "CONTAINER_NAME=kozert-db-integration PORT=54321 bash ./scripts/start-db.sh",
    "test": "jest",
    "test:integration": "DATABASE_URL=postgres://root:root@localhost:54321/db jest -c jest.config.int.js --runInBand",
    "test:coverage": "bash scripts/coverage.sh",
    "type-check": "tsc --noEmit",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "@babel/preset-env": "^7.13.15",
    "@prisma/client": "^2.21.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "next": "10.1.3",
    "next-auth": "^3.17.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.3.2",
    "winston": "^3.3.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "execa": "^4.1.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "node-mocks-http": "^1.10.1",
    "postcss": "^8.2.12",
    "prettier": "^2.2.1",
    "prisma": "^2.21.2",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4"
  }
}
